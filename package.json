{
  "name": "@spotlightdata/nanowire-extensions",
  "version": "1.2.0",
  "description": "utilities and components used in nanowire platform",
  "main": "dist/nanowire-extensions.cjs.js",
  "module": "dist/nanowire-extensions.esm.js",
  "browser": "dist/nanowire-extensions.umd.js",
  "repository": "git@github.com:SpotlightData/nanowire-extensions.git",
  "author": "Augustinas <augustinas@spotlightdata.co.uk>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "BABEL_ENV=test kcd-scripts test --config=jest.config.js --maxWorkers=4",
    "build:bundle": "rollup -c",
    "build:lib": "BABEL_ENV=lib npx babel src -d lib",
    "build": "BABEL_ENV=build npm-run-all --parallel build:bundle build:lib",
    "dev": "rollup -c -w",
    "clean-code": "prettier --write 'src/**/*.js' 'tests/**/*.js' ",
    "prepublishOnly": "yarn build",
    "packages": "yarn install && bash peer.sh"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-1": "^7.0.0-beta.51",
    "@team-griffin/install-self-peers": "^1.1.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.1",
    "jest": "^23.1.0",
    "jest-canvas-mock": "^1.0.3",
    "jest-dom": "^1.11.0",
    "jmespath": "^0.15.0",
    "kcd-scripts": "^0.39.1",
    "npm-run-all": "^4.1.3",
    "react-dom": "^16.4.2",
    "react-testing-library": "^4.0.1",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.65.1",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "wait-for-expect": "^1.0.0"
  },
  "files": [
    "dist",
    "lib"
  ],
  "peerDependencies": {
    "antd": "^3.6.3",
    "axios": "^0.18.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-jss": "^8.5.1",
    "rxjs": "^6.2.1",
    "shortid": "^2.2.8",
    "uppy": "^0.25.5"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.5.5",
    "lodash.throttle": "^4.1.1",
    "preact": "^8.2.9",
    "rc-menu": "^7.4.5",
    "react-animate-height": "^2.0.4",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-vega": "^3.1.2",
    "shallowequal": "^1.1.0",
    "vega": "^4.0.0-rc.3"
  }
}
